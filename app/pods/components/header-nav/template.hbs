<div class="hero-head">
  <nav class="navbar is-fixed-top">
    <div class="container">
      <div class="navbar-brand">
        <span class="navbar-burger burger {{isActiveClass}}" data-target="navbarMenu" onclick={{action "showMenu"}}>
          <span></span>
          <span></span>
          <span></span>
        </span>
      </div>
      <div id="navbarMenu" class="navbar-menu {{isActiveClass}}">
        <div class="navbar-end">
          {{link-to "Overview" "index" class="navbar-item" activeClass="is-active"}}

          {{#each links as |link|}}
            {{link-to link.name "content" link.path class="navbar-item" activeClass="is-active"}}
          {{/each}}
          {{#link-to "content" content.url class="navbar-item" activeClass="is-active"}}<strong>Playground</strong>
          {{/link-to}}
        </div>
      </div>
    </div>
  </nav>
</div>
